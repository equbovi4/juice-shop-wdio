{
  "name": "wdio",
  "description": "Intermediate WDIO final task",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/register": "^7.22.5",
    "@wdio/allure-reporter": "^8.14.0",
    "@wdio/cli": "^8.19.0",
    "@wdio/cucumber-framework": "^8.19.0",
    "@wdio/local-runner": "^8.19.0",
    "@wdio/spec-reporter": "^8.19.0",
    "del-cli": "^5.1.0",
    "selenium-standalone": "^9.0.3",
    "wdio-image-comparison-service": "^5.0.3",
    "wdio-selenium-standalone-service": "^0.0.12",
    "wdio-wait-for": "^3.0.7"
  },
  "scripts": {
    "wdio": "wdio run ./config/wdio.conf.js",
    "wdio:parallel": "wdio run ./config/wdio.conf-parallel.js",
    "wdio:chrome": "wdio run ./config/wdio.conf-chrome.js",
    "wdio:chrome:headless": "wdio run ./config/wdio.conf-chrome.js --headless='true'",
    "wdio:firefox": "wdio run ./config/wdio.conf-firefox.js",
    "wdio:firefox:headless": "wdio run ./config/wdio.conf-firefox.js --headless='true'",
    "wdio:edge": "wdio run ./config/wdio.conf-edge.js",
    "wdio:edge:headless": "wdio run ./config/wdio.conf-edge.js --headless='true'",
    "deleteReports": "del-cli --force ./allure-results/*"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "canvas": "^2.11.2",
    "chromedriver": "^120.0.1",
    "deepmerge": "^4.3.1"
  }
}
